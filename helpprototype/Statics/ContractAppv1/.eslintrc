{
  "parser"  : "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "React": true,
    "ReactDOM": true
  },
  "rules": {
    "semi" : [2, "always"],
    "react-in-jsx-scope": 0,
    "space-before-function-paren": 0,
    "space-after-keywords": 0,
    "space-before-blocks": 0,
    "spaced-comment": 0,
    "brace-style": 0,
    "object-curly-spacing": [2, "always"]
  }
}
